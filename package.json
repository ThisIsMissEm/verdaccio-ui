{
  "name": "ui",
  "version": "1.0.0",
  "description": "Verdaccio User Interface",
  "author": {
    "name": "Verdaccio Core Team"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/verdaccio/verdaccio"
  },
  "main": "build/index.js",
  "bin": {
    "verdaccio": "./bin/verdaccio"
  },
  "dependencies": {
